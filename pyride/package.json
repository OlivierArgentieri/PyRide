{
  "name": "pyride",
  "displayName": "PyRide",
  "description": "move along definition using alt+arrows",
  "repository": "https://github.com/OlivierArgentieri/PyRide",
  "publisher": "OlivierArgentieri",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "pyride.previousDef",
        "title": "navigate through previous definitions."
      },
      {
        "command": "pyride.nextDef",
        "title": "navigate through next definitions."
      }
    ],
    "keybindings": [
      {
        "command": "pyride.nextDef",
        "key": "alt+down"
      },
      {
        "command": "pyride.previousDef",
        "key": "alt+up"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.83.0",
    "@types/mocha": "^10.0.2",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2",
    "@vscode/test-electron": "^2.3.4"
  }
}
